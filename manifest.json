{
  "name": "Habit Tracker",
  "version": "2.1",
  "description": "simple habit tracking in your browser",
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "browser_action": {
    "default_popup": "structure.html",
    "default_icon": {
      "128": "logo.png"
    }
  },
  "icons": {
    "128": "logo.png"
  },
  "manifest_version": 2
}